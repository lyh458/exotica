<?xml version="1.0" ?>
<IKSolverDemoConfig>
  <IKSolver Name="MySolver">
    <MaxIterations>1</MaxIterations>
  </IKSolver>

  <UnconstrainedEndPoseProblem Name="MyProblem">
    <PlanningScene>
      <Scene>
        <JointGroup>manipulator</JointGroup>
        <URDF>{exotica_examples}/resources/robots/iiwa7.urdf</URDF>
        <SRDF>{exotica_examples}/resources/robots/iiwa7.srdf</SRDF>
      </Scene>
    </PlanningScene>

    <Maps>
      <EffFrame Name="Position">
        <EndEffector>
          <Frame Link="iiwa_link_7" LinkOffset="0 0 0 0 -0.707 0 0.707"/>
        </EndEffector>
      </EffFrame>

      <Manipulability Name="Manip">
        <EndEffector>
          <Frame Link="iiwa_link_7"/>
        </EndEffector>
      </Manipulability>

    </Maps>

    <Cost>
      <Task Task="Position" Rho="1"/>
      <Task Task="Manip" Rho="1"/>
    </Cost>

    <StartState>-0.0845 0.9632 -0.1941 -2.1545 -0.4535 -1.5106 0.3334</StartState>
    <!-- <NominalState>0 0 0 0 0 0 0</NominalState> -->
    <W> 7 6 5 4 3 2 1 </W>
  </UnconstrainedEndPoseProblem>
</IKSolverDemoConfig>
