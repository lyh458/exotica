<?xml version="1.0" ?>
<ExampleConfig>
  <BayesianIKSolver Name="MySolver"/>

  <UnconstrainedEndPoseProblem Name="ExampleProblem">
    <PlanningScene>
      <Scene>
        <JointGroup>manipulator</JointGroup>
        <URDF>{exotica_examples}/resources/robots/iiwa7.urdf</URDF>
        <SRDF>{exotica_examples}/resources/robots/iiwa7.srdf</SRDF>
      </Scene>
    </PlanningScene>
    <Maps>
      <EffFrame Name="Position1">
        <EndEffector>
          <Frame Link="iiwa_link_7" BaseOffset="0.7 -0.4 0.35 0 0 0 1" LinkOffset="0 0 0.2 0 -0.707 0 0.707"/>
        </EndEffector>
      </EffFrame>
      <EffFrame Name="Position2">
        <EndEffector>
          <Frame Link="iiwa_link_7" BaseOffset="0.7 0.4 0.35 0 0 0 1" LinkOffset="0 0 0.2 0 -0.707 0 0.707"/>
        </EndEffector>
      </EffFrame>
      <JointLimit Name="JointLimit"/>
      <JointPose Name="JointPose"/>
    </Maps>

    <Cost>
      <Task Task="JointLimit" Rho="1e2"/>
      <Task Task="JointPose"/>
      <Task Task="Position1"/>
      <Task Task="Position2"/>
    </Cost>

    <W> 7 6 5 4 3 2 1 </W>
    <StartState>0 0.1 0 0 0 0 0</StartState>
  </UnconstrainedEndPoseProblem>
</ExampleConfig>
