#!/usr/bin/env python

import pyexotica._pyexotica as exo
from pyexotica_modules.publish_trajectory import *

exo.Setup.init_ros()
solver = exo.Setup.load_solver(
    '{exotica_examples}/resources/configs/example_ompl_freebase_dubins.xml')

solution = solver.solve()

plot(solution)

publish_trajectory(solution, 3.0, solver.get_problem())
